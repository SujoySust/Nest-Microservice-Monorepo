{
  "collection": "@nestjs/schematics",
  "monorepo": true,
  "projects": {
    "auth-gateway-service": {
      "type": "application",
      "root": "apps/auth-gateway-service",
      "entryFile": "main",
      "sourceRoot": "apps/auth-gateway-service/src",
      "compilerOptions": {
        "webpack": true,
        "tsConfigPath": "apps/auth-gateway-service/tsconfig.app.json"
      }
    },
    "user-service": {
      "type": "application",
      "root": "apps/user-service",
      "entryFile": "main",
      "sourceRoot": "apps/user-service/src",
      "compilerOptions": {
        "webpack": true,
        "tsConfigPath": "apps/user-service/tsconfig.app.json"
      }
    },
    "logger-service": {
      "type": "application",
      "root": "apps/logger-service",
      "entryFile": "main",
      "sourceRoot": "apps/logger-service/src",
      "compilerOptions": {
        "webpack": true,
        "tsConfigPath": "apps/logger-service/tsconfig.app.json"
      }
    },
    "notification-service": {
      "type": "application",
      "root": "apps/notification-service",
      "entryFile": "main",
      "sourceRoot": "apps/notification-service/src",
      "compilerOptions": {
        "webpack": true,
        "tsConfigPath": "apps/notification-service/tsconfig.app.json"
      }
    },
    "prisma": {
      "type": "library",
      "root": "libs/prisma",
      "entryFile": "index",
      "sourceRoot": "libs/prisma/src",
      "compilerOptions": {
        "tsConfigPath": "libs/prisma/tsconfig.lib.json"
      }
    }
  }
}
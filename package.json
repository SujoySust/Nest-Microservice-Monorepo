{
  "name": "nest-microservice-monorepo",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "postgres:generate": "cd libs/prisma/postgres/schema && prisma generate",
    "postgres:push": "cd libs/prisma/postgres/schema && prisma db push",
    "postgres:seed": "cd libs/prisma/postgres/schema && prisma db seed",
    "mongo:push": "cd libs/prisma/mongo/schema && prisma db push",
    "mongo:generate": "cd libs/prisma/mongo/schema && prisma generate",
    "mongo:seed": "cd libs/prisma/mongo/schema && prisma db seed",
    "migrate": "cd libs/prisma/mongo/schema && prisma db push && cd ../../mongo/schema && prisma db push",
    "seed": "cd libs/prisma/postgres/schema && prisma db seed && cd ../../mongo/schema && prisma db seed ",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/auth-gateway-service/test/jest-e2e.json"
  },
  "prisma": {
    "seed": "ts-node ../seeds/seed.ts"
  },
  "dependencies": {
    "@devoxa/prisma-relay-cursor-connection": "^3.1.1",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^8.0.0",
    "@nestjs/graphql": "^12.1.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/throttler": "^5.1.2",
    "@prisma/client": "5.13.0",
    "@squareboat/nestjs-localization": "^0.0.5",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.14.1",
    "device-detector-js": "^3.0.3",
    "dotenv": "^16.4.5",
    "googleapis": "^137.1.0",
    "graphql": "^16.8.1",
    "graphql-redis-subscriptions": "^2.6.1",
    "graphql-upload": "^12.0.0",
    "handlebars": "^4.7.8",
    "nestjs-prisma": "^0.23.0",
    "nestjs-throttler-storage-redis": "^0.4.4",
    "nestjs-twilio": "^4.4.0",
    "otplib": "^12.0.1",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "twilio": "^5.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/graphql-upload": "^8.0.7",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "prisma": "5.13.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/prisma(|/.*)$": "<rootDir>/libs/prisma/src/$1"
    }
  }
}
